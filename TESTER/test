local plr = game:GetService("Players").LocalPlayer
local char = plr.Character or plr.CharacterAdded:Wait()
local rleg = char:FindFirstChild("RightBoot") or print("RightBoot not found")
local lleg = char:FindFirstChild("LeftBoot") or print("LeftBoot not found")


local camera = game:GetService("Workspace").CurrentCamera
local studs = 25

local props = {
    size = Vector3.new(1.75, 1.75, 1.75),
    shape = Enum.PartType.Ball,
    material = Enum.Material.SmoothPlastic
}


local function findBalls()
    local result = {}
    for _, i in pairs(workspace:GetDescendants()) do
        if i:IsA("BasePart") then
            if i.Size == props.size and i.Material == props.material and i.Shape == props.shape then
                table.insert(result, i)
            end
        end
    end
    return result
end

local balls = findBalls()
for _, ball in ipairs(balls) do
print(ball.Name)
end
